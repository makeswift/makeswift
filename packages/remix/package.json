{
  "name": "@makeswift/remix",
  "version": "0.1.0",
  "description": "Remix adapter for Makeswift",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "eslint",
    "clean": "rm -rf .turbo node_modules dist"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "@makeswift/runtime": "workspace:*",
    "@remix-run/node": "^2.0.0"
  },
  "peerDependencies": {
    "@react-router/node": "^7.0.0",
    "react-router": "^7.0.0",
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0"
  },
  "devDependencies": {
    "@types/react": "^18.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0"
  }
}