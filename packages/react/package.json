    "@types/react": "^18.2.14",
    "@types/react-dom": "^18.2.6",
    "eslint": "^9.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "ts-jest": "^29.1.0",
    "tsup": "^8.0.1",
    "typescript": "^5.1.6"
